seq_date2 <- function (begin, end, freq2) {
start <- lubridate::as_date(begin)
end <- lubridate::as_date(end)
i <- lubridate::interval(begin, end)
nb_pts <- floor(i / freq2)
begin + 1:nb_pts * freq2
}
